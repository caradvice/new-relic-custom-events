{
    "name": "caradvice/new-relic-custom-events",
    "description": "Package to send custom events to New Relic Insights",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": ">6.5",
        "illuminate/support": ">6.0",
        "illuminate/events": ">6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "CarAdvice\\NewRelic\\CustomEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CarAdvice\\NewRelic\\CustomEvents\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarAdvice\\NewRelic\\CustomEvents\\LaravelServiceProvider"
            ]
        }
    }
}
